{
    "opts": {
      "template": "./jsdoc-theme"
    },
    "plugins": [
      "plugins/markdown",
      "./jsdoc-plugins/customElementTag"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    }
}
